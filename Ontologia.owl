    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlTitle"/>
        <IRI>#CEK-06</IRI>
        <Literal>Encryption Change Cost Benefit Analysis</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlSpecification"/>
        <IRI>#CEK-07</IRI>
