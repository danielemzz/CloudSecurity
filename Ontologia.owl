    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlTitle"/>
        <IRI>#DCS-04</IRI>
        <Literal>Secure Media Transportation Policy and Procedures</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlSpecification"/>
