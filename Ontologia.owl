    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlTitle"/>
        <IRI>#CEK-03</IRI>
        <Literal>Data Encryption</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlSpecification"/>
        <IRI>#CEK-04</IRI>
