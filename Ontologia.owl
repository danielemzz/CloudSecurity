    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlTitle"/>
        <IRI>#CEK-05</IRI>
        <Literal>Encryption Change Management</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:ControlSpecification"/>
        <IRI>#CEK-06</IRI>
